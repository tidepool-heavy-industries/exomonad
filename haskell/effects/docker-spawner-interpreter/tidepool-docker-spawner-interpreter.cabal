cabal-version: 3.0
name:          tidepool-docker-spawner-interpreter
version:       0.1.0.0
synopsis:      HTTP interpreter for DockerSpawner effect

library
  exposed-modules:
    Tidepool.DockerSpawner.Interpreter
  build-depends:
    base >= 4.14 && < 5,
    tidepool-core,
    aeson,
    http-client,
    http-types,
    text,
    freer-simple,
    mtl
  hs-source-dirs: src
  default-language: Haskell2010
  ghc-options: -Wall
